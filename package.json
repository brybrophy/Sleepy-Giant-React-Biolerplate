{
    "name": "high-seas-ts-express-react-starter",
    "version": "1.0.0",
    "description": "A starter project using Exress, React, and TypeScript",
    "main": "src/index.js",
    "scripts": {
        "start:dev":
            "cross-env NODE_ENV=development ts-node src/server/server.ts",
        "start:prod":
            "cross-env NODE_ENV=production ts-node src/server/server.ts --fast",
        "build:dev": "rimraf ./dist && cross-env NODE_ENV=development webpack",
        "build:prod":
            "rimraf ./dist && cross-env NODE_ENV=production webpack -p",
        "precommit": "lint-staged",
        "prettier":
            "prettier --write --config .prettierrc \"**/*.{js,jsx,ts,tsx,json,css,scss}\""
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json,css,scss}": ["yarn run prettier", "git add"]
    },
    "author": "Bryan Brophy",
    "license": "MIT",
    "devDependencies": {
        "assets-webpack-plugin": "^3.5.1",
        "autoprefixer": "^7.1.6",
        "awesome-typescript-loader": "^3.3.0",
        "case-sensitive-paths-webpack-plugin": "^2.1.1",
        "chai": "^4.1.2",
        "chalk": "^2.3.0",
        "copy-webpack-plugin": "^4.2.0",
        "cross-env": "^5.1.1",
        "css-loader": "^0.28.7",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "husky": "^0.14.3",
        "image-webpack-loader": "^3.4.2",
        "json-loader": "^0.5.7",
        "lint-staged": "^6.0.0",
        "mocha": "^4.0.1",
        "node-sass": "^4.6.1",
        "postcss-loader": "^2.0.8",
        "precss": "^2.0.0",
        "progress-bar-webpack-plugin": "^1.10.0",
        "react-hot-loader": "^3.1.2",
        "resolve-url-loader": "^2.2.0",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.6",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.19.0",
        "typescript": "^2.6.1",
        "url-loader": "^0.6.2",
        "webpack": "^3.8.1",
        "webpack-config-utils": "^2.3.0",
        "webpack-dev-middleware": "^1.12.0",
        "webpack-hot-middleware": "^2.20.0"
    },
    "dependencies": {
        "@types/body-parser": "^1.16.8",
        "@types/circular-json": "^0.4.0",
        "@types/classnames": "^2.2.3",
        "@types/compression": "^0.0.35",
        "@types/cookie-parser": "^1.4.1",
        "@types/dotenv": "^4.0.2",
        "@types/express": "^4.0.39",
        "@types/lodash": "^4.14.85",
        "@types/moment": "^2.13.0",
        "@types/morgan": "^1.7.35",
        "@types/query-string": "^5.0.1",
        "@types/react": "^16.0.22",
        "@types/react-dom": "^16.0.3",
        "@types/react-router-dom": "^4.2.1",
        "@types/validatorjs": "^3.7.1",
        "body-parser": "^1.18.2",
        "circular-json": "^0.4.0",
        "classnames": "^2.2.5",
        "compression": "^1.7.1",
        "cookie-parser": "^1.4.3",
        "css-declaration-sorter": "^3.0.0",

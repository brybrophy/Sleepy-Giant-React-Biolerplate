        "@types/react-hot-loader": "^4.1.0",
        "@types/react-router-dom": "^4.2.7",
        "@types/rimraf": "^2.0.2",
        "@types/universal-analytics": "^0.4.2",
        "@types/validator": "^9.4.1",
        "autoprefixer": "^7.1.6",
